<p id="notice"><%= notice %></p>

<h1>Todo1 Lists</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @todo1_lists.each do |todo1_list| %>
      <tr>
        <td><%= todo1_list.title %></td>
        <td><%= todo1_list.description %></td>
        <td><%= link_to 'Show', todo1_list %></td>
        <td><%= link_to 'Edit', edit_todo1_list_path(todo1_list) %></td>
        <td><%= link_to 'Destroy', todo1_list, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Todo1 List', new_todo1_list_path %>
